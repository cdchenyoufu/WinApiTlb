	const long ERROR_NO_RECOVERY_POLICY         = 6003;

    [ helpstring("The required encryption driver is not loaded for this system") ]
	const long ERROR_NO_EFS                     = 6004;

    [ helpstring("The file was encrypted with a different encryption driver than is currently loaded") ]
	const long ERROR_WRONG_EFS                  = 6005;

    [ helpstring("There are no EFS keys defined for the user") ]
	const long ERROR_NO_USER_KEYS               = 6006;

    [ helpstring("The specified file is not encrypted") ]
	const long ERROR_FILE_NOT_ENCRYPTED         = 6007;

    [ helpstring("The specified file is not in the defined EFS export format") ]
	const long ERROR_NOT_EXPORT_FORMAT          = 6008;

	// Success codes (from Structured Storage and other COM functions)
    [ helpstring("COM function returned Success or True") ]
    const long S_OK                             = 0;
    [ helpstring("COM function returned False") ]
    const long S_FALSE                          = 0x00000001L;

